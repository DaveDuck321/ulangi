    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "expose-loader": "^1.0.0",

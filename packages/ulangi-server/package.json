    "@types/app-root-path": "^1.2.4",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.6",
    "@types/elasticsearch": "^5.0.36",
    "@types/express": "^4.17.6",
    "@types/google-cloud__text-to-speech": "^0.5.0",
    "@types/hashids": "^1.0.30",
    "@types/in-app-purchase": "^1.11.0",
    "@types/jest": "^26.0.3",
    "@types/joi": "^14.3.4",
    "@types/js-yaml": "^3.12.5",
    "@types/jsonfile": "^6.0.0",
